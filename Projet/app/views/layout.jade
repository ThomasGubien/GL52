              a.dropdown-toggle(href='#', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                i.menu-icon.fa.fa-book
                | Projects
              ul.sub-menu.children.dropdown-menu
                li
                  i.fa.fa-table
                  a(href='/projects/getList') List
                li
                  i.fa.fa-plus
                  a(href='/projects/create') Create
            h3.menu-title 
            // /.menu-title
            li.menu-item-has-children.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                i.menu-icon.fa.fa-files-o
                | Documents
              ul.sub-menu.children.dropdown-menu
                li
                  i.menu-icon.fa.fa-upload
                  a(href='/file/upload') Upload
                li
                  i.menu-icon.fa.fa-download
                  a(href='/file/download') Download
            h3.menu-title 
            // /.menu-title
            li.menu-item-has-children.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
